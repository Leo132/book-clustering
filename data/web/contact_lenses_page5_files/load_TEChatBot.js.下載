document.addEventListener('DOMContentLoaded', function() {

	// 動態載入客服
	setTimeout(function () {
		var t0 = document.createElement("link");
		t0.type = "text/css", t0.rel = "stylesheet", t0.href = "https://TEChatBot.telexpress.com/TEChatBot-EYESmart/Styles/te_ts_clientChat_shrink.css";
		// t0.onload = function(){ console.log('t0') }
		document.getElementsByTagName("head")[0].appendChild(t0)

		var t1 = document.createElement("script");
		t1.type = "text/javascript", t1.src = "https://TEChatBot.telexpress.com/TEChatBot-EYESmart/Scripts/temcs_chat_client.js";
		t1.onload = function(){
			// console.log('t1')

			if (typeof Temcs === 'function') {
				new Temcs({
					domain: 'https://TEChatBot.telexpress.com/TEChatBot-EYESmart',
					type: 'iframe',
					channel: 'DEDBAD4E-C238-46C6-B87E-26A56D00BE05',
					paramterTarget: '#teparameters',
				})
			}
		}
		document.getElementsByTagName("head")[0].appendChild(t1)

		// var t2 = document.createElement("script");
		// t2.type = "text/javascript", t2.src = "https://TEChatBot.telexpress.com/TEChatBot-EYESmart/Scripts/jquery-1.10.2.js";
		// t2.onload = function () {
		// 	// console.log('t2')
		// 	if (typeof Temcs === 'function') {
		// 		new Temcs({
		// 			domain: 'https://TEChatBot.telexpress.com/TEChatBot-EYESmart',
		// 			type: 'iframe',
		// 			channel: 'DEDBAD4E-C238-46C6-B87E-26A56D00BE05',
		// 			paramterTarget: '#teparameters',
		// 		})
		// 	}
		// }
		// document.getElementsByTagName("head")[0].appendChild(t2)
	}, 2000);
});